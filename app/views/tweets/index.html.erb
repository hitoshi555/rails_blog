<div class="contents row" >
    <div>
        <% @tweets.each do |tweet| %>
            <div class="col s12 m6 l3">
                <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                    <span class="card-title"><%= tweet.id %></span>
                    <p><%= tweet.text %></p>
                </div>
                </div>
            </div>
        <% end %>
    </div>
</div>